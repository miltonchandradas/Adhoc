<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Cancel Correspondence"
		class="sapUiPopupWithPadding"
		items="{/}"
		search="_handleMultiValueHelpSearch"
		confirm="_handleMultiValueHelpClose"
		cancel="_handleMultiValueHelpClose"
		multiSelect="true">
		<StandardListItem
			title="{title}"
			description="Completed: {completed}" />
	</SelectDialog>
</core:FragmentDefinition>